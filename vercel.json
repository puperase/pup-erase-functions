{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ],
  "redirects": [{ "source": "/", "destination": "/api/check" }],
  "crons": [
    {
      "path": "/api/run",
      "schedule": "0 0 * * *"
    }
  ]
}
